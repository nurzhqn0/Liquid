{
  "manifest_version": 3,
  "name": "Liquid",
  "version": "0.1.0",
  "description": "Highlight text and get a hint + explanation via AI (for study/practice).",
  "permissions": [],
  "host_permissions": ["http://localhost:3001/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}
